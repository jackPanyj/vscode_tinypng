{
  "name": "tinypng",
  "displayName": "tinypng",
  "description": "tiny png with many keys",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.20.0"
  },
  "repository": {
    "url": "https://github.com/jackPanyj/vscode_tinypng"
  },
  "icon": "panda-happy.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.compressFile",
    "onCommand:extension.compressFolder"
  ],
  "main": "./out/extension.js",
  "publisher": "jackpanyj",
  "contributes": {
    "configuration": {
      "title": "tinypng config",
      "properties": {
        "tinypng.apiKeys": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "description": "tinypng api key"
        }
      }
    },
    "commands": [
      {
        "command": "extension.compressFile",
        "title": "Tinypng: compress this image"
      },
      {
        "command": "extension.compressFolder",
        "title": "Tinypng: compress images here"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.compressFile",
          "when": "resourceLangId == vscode_tinypng",
          "group": "navigation"
        },
        {
          "command": "extension.compressFolder",
          "when": "explorerResourceIsFolder",
          "group": "navigation"
        }
      ]
    },
    "languages": [
      {
        "id": "vscode_tinypng",
        "extensions": [
          ".png",
          ".jpg",
          ".jpeg"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "tinify": "^1.6.0-beta.2"
  },
  "__npminstall_done": false
}
